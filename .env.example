# Configuration de base de données (pour développement local)
# Sur Render, DATABASE_URL est automatiquement fourni

# PostgreSQL local (pour développement)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=mse_reports
DB_USER=postgres
DB_PASSWORD=

# PHP Configuration
PHP_DISPLAY_ERRORS=Off
PHP_ERROR_REPORTING=E_ALL & ~E_DEPRECATED & ~E_STRICT

# Session
SESSION_SAVE_PATH=/tmp/sessions

# Application
APP_ENV=production
APP_DEBUG=false
